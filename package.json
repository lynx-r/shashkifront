  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "cli": "ng",
    "start": "ng serve",
    "local": "sudo yarn run cli serve --no-aot --port 80 --disable-host-check",
    "prod": "yarn cli build --prod --base-href \"/\" --output-path \"wiki-dist/wiki-app\"",
    "appshell": "ng run shashkiwiki:app-shell:production",
    "deploy": "cd dist && rm -f shashkionline.zip && zip -r shashkionline.zip shashkionline && scp shashkionline.zip ruvds:~ && ssh ruvds \"sh ~/deploy.sh\"",
    "appshellanddeploy": "yarn appshell && yarn deploy",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
